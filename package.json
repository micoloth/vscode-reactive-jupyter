{
	"name": "reactivejupyter",
	"displayName": "Reactive Jupyter",
	"description": "Jupyter notebook support, with Reactive interactive programming and computing and support for Intellisense, debugging, and more.",
	"publisher": "micoloth",
	"author": {
		"name": "Mike Tasca"
	},
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.86.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"jupyter",
		"reactive",
		"notebook",
		"notebookKernelJupyterNotebook",
		"multi-root ready",
		"python"
	],
	"activationEvents": [
		"onNotebook:jupyter-notebook",
		"onNotebook:interactive",
		"onLanguage:python"
	],
	"extensionDependencies": [
		"ms-toolsai.jupyter"
	],
	"main": "./out/extension.js",
	"contributes": {
		"colors": [
			{
				"id": "reactivejupyter.syncedCell",
				"description": "Background decoration color for synced unselected cell",
				"defaults": {
					"dark": "#0dca3957",
					"light": "#0dca3957",
					"highContrast": "#0dca3957"
				}
			},
			{
				"id": "reactivejupyter.syncedCurrentCell",
				"description": "Background decoration color for synced unselected cell",
				"defaults": {
					"dark": "#0dca39c8",
					"light": "#0dca39c8",
					"highContrast": "#0dca39c8"
				}
			},
			{
				"id": "reactivejupyter.outdatedCell",
				"description": "Background decoration color for current query",
				"defaults": {
					"dark": "#d4164658",
					"light": "#d4164658",
					"highContrast": "#d4164658"
				}
			},
			{
				"id": "reactivejupyter.outdatedCurrentCell",
				"description": "Background decoration color for current query",
				"defaults": {
					"dark": "#d41646da",
					"light": "#d41646da",
					"highContrast": "#d41646da"
				}
			}
		],
		"commands": [
			{
				"command": "jupyterKernelExecution.listKernels",
				"title": "Execute code against a Python Kernel",
				"category": "Jupyter Kernel API"
			},
			{
				"command": "reactivejupyter.initialize-reactive-python-extension",
				"title": "Initialize Reactive Python",
				"category": "Reactive Python"
			},
			{
				"command": "reactivejupyter.sync-downstream",
				"title": "Refresh all Downstream",
				"category": "Reactive Python"
			},
			{
				"command": "reactivejupyter.sync-upstream",
				"title": "Refresh all Upstream",
				"category": "Reactive Python"
			},
			{
				"command": "reactivejupyter.sync-upstream-and-downstream",
				"title": "Refresh all Upstream and Downstream of the current cell",
				"category": "Reactive Python"
			},
			{
				"command": "reactivejupyter.sync-all",
				"title": "Refresh all Stale Blocks",
				"category": "Reactive Python"
			},
			{
				"command": "reactivejupyter.sync-current",
				"title": "Run current block",
				"category": "Reactive Python"
			},
			{
				"command": "reactivejupyter.test-command",
				"title": "Refresh all Downstream",
				"category": "Reactive Python"
			}
		],
		"keybindings": [
            {
                "command": "reactivejupyter.sync-all",
                "key": "shift+cmd+enter",
                "when": "isWorkspaceTrusted && editorTextFocus && editorLangId == python && !notebookEditorFocused"
            },
            {
                "command": "reactivejupyter.sync-upstream",
                "key": "shift+cmd+up shift+cmd+enter",
                "when": "isWorkspaceTrusted && editorTextFocus && editorLangId == python && !notebookEditorFocused"
            },
            {
                "command": "reactivejupyter.sync-downstream",
                "key": "shift+cmd+down shift+cmd+enter",
                "when": "isWorkspaceTrusted && editorTextFocus && editorLangId == python && !notebookEditorFocused"
            },
            {
                "command": "reactivejupyter.sync-current",
                "key": "shift+enter",
                "when": "isWorkspaceTrusted && editorTextFocus && editorLangId == python && !notebookEditorFocused"
            }
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -b",
		"lint": "eslint src --ext ts",
		"watch": "tsc -b --watch"
	},
	"devDependencies": {
		"@types/node": "14.x",
		"@types/uuid": "^3.4.3",
		"@types/vscode": "^1.82.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"@vscode/jupyter-extension": "^1.1.0",
		"eslint": "^7.27.0",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"@vscode/python-extension": "^1.0.5",
		"uuid": "^9.0.1",
        "@c4312/evt": "^0.1.1",
        "fast-deep-equal": "^2.0.1",
        "@jupyterlab/nbformat": "3.1.17",
        "@jupyterlab/services": "6.1.17"
	}
}
